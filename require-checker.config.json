{
  "symbol-whitelist": [
    "null", "true", "false",
    "static", "self", "parent",
    "array", "string", "int", "float", "bool", "iterable", "callable", "void", "object",
    "wp_clear_scheduled_hook", "wp_next_scheduled", "wp_schedule_event"
  ],
  "php-core-extensions": [
    "Core",
    "date",
    "pcre",
    "Phar",
    "Reflection",
    "SPL",
    "standard"
  ],
  "scan-files": []
}
